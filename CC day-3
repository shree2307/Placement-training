#include <stdio.h>

int houses_suff(int r,int unit,int houses[],int len)
{
    int totalFood=r*unit;
    int sumFood=0;
    int houseCount=0;
    for(int i=0;i<len;i++)
    {
        sumFood+=houses[i];
        houseCount++;
        if(sumFood>=totalFood)
        {
            break;
        }
    }
    return houseCount;
}

void bubbleSort(int houses[], int len)
{
    for (int i = 0; i < len - 1; i++) {
        int swapped = 0;
        for (int j = 0; j < len - i - 1; j++) {
            if (houses[j] < houses[j + 1]) {
                int temp = houses[j];
                houses[j] = houses[j + 1];
                houses[j + 1] = temp;
                swapped = 1;
            }
        }
        if (swapped == 0) {
            break;
        }
    }



